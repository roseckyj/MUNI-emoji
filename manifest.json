{
    "manifest_version": 2,
    "name": "MUNI emoji",
    "description": "Používejte na messengeru emoji, jako v ISu... Otravné!",
    "author": "Jonas Rosecky",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "http://*.messenger.com/*",
                "https://*.messenger.com/*"
            ],
            "js": [
                "src/muniemoji.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "48": "src/icon_48.png",
        "64": "src/icon_64.png"
    }
}
